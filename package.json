{
  "name": "entry-level",
  "version": "1.1.3",
  "description": "Basic Electron app skeleton for school project",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "dist-win": "electron-builder --win --x64 --config.win.target=nsis",
    "dist-mac": "electron-builder --mac"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "electron": "^26.2.0",
    "electron-builder": "^26.0.12"
  },
  "build": {
    "appId": "com.example.entrylevel",
    "productName": "Entry Level",
    "icon": "logopng.png",
    "files": [
      "**/*"
    ],
    "win": {
      "target": "nsis"
    },
    "mac": {
      "icon": "icons/icon.icns",
      "target": [
        "dmg",
        "zip"
      ],
      "category": "public.app-category.productivity"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Ucloptas/Entry-Level.git"
  },
  "bugs": {
    "url": "https://github.com/Ucloptas/Entry-Level/issues"
  },
  "homepage": "https://github.com/Ucloptas/Entry-Level#readme"
}
